
//fixed
#include<stdio.h>
int main(){
    int arr[90],n,i,*p,*q,arr2[98],sum = 0;

    printf("enter n =");
    scanf("%d",&n);
    printf("enter array:\n");

    for(i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }

    p = &arr[0];
    


    for(i=0;i<n;i++){
        sum +=  *(p+i);

    }
    printf("sum is %d : ",sum);
   
}
